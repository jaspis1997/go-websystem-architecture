	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${fileDirname}"
		},
		{
			"name": "Launch Package(cmd)",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"cwd": "${workspaceFolder}",
			"program": "./cmd",
			"buildFlags": ["-trimpath"],
			"args": [],
			"env": {}
		},
		{
			"name": "Launch Package (views)",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"cwd": "${workspaceFolder}",
			"program": "./test/views/",
			"buildFlags": ["-trimpath"],
			"args": [],
			"env": {
				"PORT": "19565",
				"VIEWS_ROOT_PATH": "./views",
				"TEMPLATE_ROOT_PATH": "views/dist"
			}
		}
	]
}
